{
  "name": "generator-p4",
  "version": "3.0.1",
  "description": "Yeoman generator for Foundation 6 + Sass + Gulp + Webpack + Composer + PHP",
  "homepage": "https://github.com/williambeto/generator-p4",
  "author": "Jos√© Willams <williambeto@hotmail.com> (https://josewillams.com/)",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "foundation 6",
    "zurb",
    "foundation",
    "zurb foundation 6",
    "sass",
    "scss",
    "libsass",
    "gulp",
    "webpack",
    "browsersync",
    "responsive",
    "front-end",
    "framework",
    "composer",
    "php"
  ],
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.0.2",
    "prettier": "^1.18.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "engines": {
    "npm": ">= 6.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^4.0.1",
    "chalk": "^2.4.2",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williambeto/generator-p4.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/williambeto/generator-p4/issues"
  }
}
